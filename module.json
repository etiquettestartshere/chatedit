{
  "id": "chatedit",
  "title": "Chat Edit",
  "version": "0.0.3",
  "scripts": ["libs/marked.min.js"],
  "esmodules": ["setup.mjs"],
  "styles": ["styles/chatedit-styles.css"],
  "compatibility": {
    "minimum": "11",
    "verified": "12"
  },
  "languages": [
    {"lang": "en", "name": "English", "path": "lang/en.json"}
  ],
  "flags": {"hotReload": {"extensions": ["css", "json", "hbs"], "paths": ["styles", "lang", "templates"]}},
  "relationships": {
    "requires": [
        {
            "id": "lib-wrapper",
            "compatibility": {
                "minimum": "1.12.7.1"
            }
        }
    ]
  }
}